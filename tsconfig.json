{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src/app"
  ],
  "paths": {
    "@app/*": [
        "src/app/*"
    ],
    "@assets/*": [
        "src/assets/*"
    ],
    "@core/*": [
        "src/app/core/*"
    ],
    "@shared/*": [
        "src/app/shared/*"
    ]
}
}
